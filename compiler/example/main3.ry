# cond := 0
# cond and (print "yes" or 1) or print "no"

else := [expr num] -> num {}
if := [cond num] -> num {
	(
		cond
		and ([expr num] -> num {
			[expr num] -> num {
				[] -> num {}
			}
		})
		or ([] -> num {
			[expr num] -> num {
				expr
			}
		})
	)
}

